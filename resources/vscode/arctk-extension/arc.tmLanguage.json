        "generic": {
            "patterns": [
                {
                    "name": "arc.generic.typename",
                    "match": "(?<![a-zA-Z0-9_.-])[A-Z][_LR]?(?![a-zA-Z0-9_.-])"
                }
            ]
        },
        "naming": {
            "patterns": [
                {
                    "name": "arc.naming.snake-case.leading-underscore",
                    "match": "\\b(_[a-z][a-z0-9_]*)\\b(?<!_)"
                },
                {
                    "name": "arc.naming.snake-case.trailing-underscore",
                    "match": "(?!_)\\b([a-z0-9_]*_)\\b"
                },
                {
                    "name": "arc.naming.constant",
                    "match": "[A-Z_][A-Z0-9_]+"
                }
            ]
        },
        "sections": {
            "patterns": [
                {
                    "name": "arc.section.block",
                    "match": "/\\*(.|[\r\n])*?\\*/(?m)"
                },
                {
                    "name": "arc.section.title",
                    "match": "\/\/  == .+ =="
                },
                {
                    "name": "arc.section.subtitle",
                    "match": "\/\/  -- .+ --"
                },
                {
                    "name": "arc.section.guard",
                    "match": "\/\/ [A-Z_]+"
                }
            ]
        }
    },
    "scopeName": "arc.injection"
